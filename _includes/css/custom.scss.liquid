@import "./custom/custom";

// https://github.com/just-the-docs/just-the-docs/blob/master/_sass/typography.scss

h4,
.text-delta {
  @include fs-3;
  color: $grey-dk-200;
}

h5,
.text-epsilon {
  @include fs-2;
  color: $grey-dk-200;
}

h6,
.text-zeta {
  @include fs-2;
  font-weight: 400;
  text-transform: uppercase;
  letter-spacing: 0.1em;
}

h6 code {
  text-transform: none;
}
